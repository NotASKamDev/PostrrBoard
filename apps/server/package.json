{
     "name": "server",
     "packageManager": "yarn@4.1.1",
     "devDependencies": {
          "@hyrious/esbuild-plugin-commonjs": "^0.2.4",
          "@types/express": "^4.17.21",
          "@types/mongoose": "^5.11.97",
          "@types/node": "^20.11.30",
          "@types/passport": "^1.0.16",
          "@types/rollup": "^0.54.0",
          "concurrently": "^8.2.2",
          "esbuild": "0.19.2",
          "esbuild-plugin-node-externals": "^1.0.1",
          "mongodb-memory-server": "^9.1.7",
          "nodemon": "^3.1.0",
          "prettier": "^3.2.5",
          "tsc-alias": "^1.8.8",
          "tsup": "^8.0.2",
          "typescript": "^5.4.3"
     },
     "dependencies": {
          "bcrypt": "^5.1.1",
          "dotenv": "^16.4.5",
          "express": "^4.19.1",
          "mongoose": "^8.2.3",
          "passport": "^0.7.0",
          "pino": "^8.19.0",
          "pino-http": "^9.0.0",
          "pino-pretty": "^11.0.0",
          "zod": "^3.22.4"
     },
     "type": "module",
     "main": "dist/index.js",
     "scripts": {
          "d": "yarn dev",
          "dev": "npx nodemon -y -e ts --watch src --exec 'yarn lint && yarn dev:build && yarn dev:start' || exit 1",
          "dev:start": "node dist/index.js",
          "dev:build": "tsup --config tsup.config.ts",
          "lint": "tsc",
          "dd": "yarn docker:dev",
          "docker:dev": "cd ../../Docker && echo $PWD && docker-compose -f ./dev.docker-compose.yml build --no-cache=true && docker compose -f ./dev.docker-compose.yml up --force-recreate -d --build ",
          "ddb": "yarn docker:db-only",
          "docker:db-only": "cd ../../Docker && docker compose -f ./dev-db.docker-compose.yml up --force-recreate -d --build",
          "dev:mem": "USE_MEMSERV=true yarn d",
          "md": "yarn dev:mem"
     }
}
