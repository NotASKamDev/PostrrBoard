{
     "name": "server",
     "packageManager": "yarn@4.1.1",
     "devDependencies": {
          "@types/bcrypt": "^5.0.2",
          "@types/express": "^4.17.21",
          "@types/mongoose": "^5.11.97",
          "@types/node": "^20.11.30",
          "@types/passport": "^1.0.16",
          "concurrently": "^8.2.2",
          "esbuild": "^0.20.2",
          "mongodb-memory-server": "^9.1.7",
          "nodemon": "^3.1.0",
          "prettier": "^3.2.5",
          "ts-node": "^10.9.2",
          "tsc-alias": "^1.8.8",
          "tsc-watch": "^6.1.0",
          "typescript": "^5.4.3",
          "vitest": "^1.4.0"
     },
     "dependencies": {
          "argon2": "^0.40.1",
          "bcrypt": "^5.1.1",
          "dotenv": "^16.4.5",
          "express": "^4.19.1",
          "mongoose": "^8.2.4",
          "passport": "^0.7.0",
          "pino": "^8.19.0",
          "pino-http": "^9.0.0",
          "pino-pretty": "^11.0.0",
          "zod": "^3.22.4"
     },
     "type": "module",
     "main": "dist/index.js",
     "scripts": {
          "d": "yarn dev",
          "build": "yarn lint && node esbuild.prod.config.js",
          "dev": "npx tsc-watch --onSuccess \"yarn dev:watch\"",
          "dev:watch": "yarn dev:build && yarn dev:start dist/index.js",
          "dev:start": "node dist/index.js",
          "dev:build": "node esbuild.dev.config.js",
          "lint": "tsc",
          "dd": "yarn docker:dev",
          "docker:dev": "cd ../../Docker && echo $PWD && docker-compose -f ./dev.docker-compose.yml build --no-cache=true && docker compose -f ./dev.docker-compose.yml up --force-recreate -d --build ",
          "ddb": "yarn docker:db-only",
          "docker:db-only": "cd ../../Docker && docker compose -f ./dev-db.docker-compose.yml up --force-recreate -d --build",
          "dev:mem": "USE_MEMSERV=true yarn d",
          "md": "yarn dev:mem"
     }
}
